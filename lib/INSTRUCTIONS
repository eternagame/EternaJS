### VIENNA 2.1.8
################
## This is how the patch file was created.
## diff -ur ViennaRNA-2.1.8 ViennaRNA-2.1.8_patched > vrna2_patch

patch -t -p0 < vrna2_patch
cp vrna2_CML.txt ViennaRNA-2.1.8/CMakeLists.txt
cp -r vrna2_emscripten_bindings ViennaRNA-2.1.8/emscripten
cp -r ./libsvm-2.91 ViennaRNA-2.1.8
pushd ViennaRNA-2.1.8
./configure
popd


### VIENNA 1.8.5
################
## This is how the patch file was created.
## diff -ur ViennaRNA-1.8.5 ViennaRNA-1.8.5_patched > vienna_patch

patch -t -p0 < vienna_patch
cp vienna_CML.txt ViennaRNA-1.8.5/CMakeLists.txt
cp -r vienna_emscripten_bindings ViennaRNA-1.8.5/emscripten
cp vienna_header.h ViennaRNA-1.8.5/RNAfold.h
pushd ViennaRNA-1.8.5
./configure
popd


### NUPACK
##########
## This is how the patch file was created.
## diff -ur nupack3.0.4_stock nupack3.0.4 > nupack_patch

# With your fresh download, unzipped into nupack3.0.4...
patch -t -p0 < nupack_patch
cp nupack_CML.txt nupack3.0.4/CMakeLists.txt
cp -r nupack_emscripten_bindings nupack3.0.4/emscripten


### LinearFold
##############
## This is how the patch file was created.
## diff -ur LinearFoldGit LinearFold  > linearfold_patch

# With your fresh git checkout of LinearFold/LinearFold...
patch -t -p0 < linearfold_patch
cp linearfold_CML.txt LinearFold/CMakeLists.txt
cp -r linearfold_emscripten_bindings LinearFold/emscripten
cp leh LinearFold/LinearFoldEval.h
