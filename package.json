{
  "name": "eternajs",
  "version": "1.0.0",
  "description": "TypeScript port of the Eterna ActionScript game",
  "scripts": {
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "rootDir": "src",
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx|js)$",
    "globals": {
      "ts-jest": {
        "skipBabel": true,
        "tsConfigFile": "../tsconfig.jest.json"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EteRNAgame/JSport.git"
  },
  "keywords": [],
  "author": "Tim Conkling",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.55",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-typescript": "^7.0.0-beta.55",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.115",
    "@types/loglevel": "^1.5.3",
    "@types/node": "^10.5.4",
    "@types/pixi.js": "^4.8.0",
    "@types/storejs": "^2.0.3",
    "@types/webfontloader": "^1.6.29",
    "babel-loader": "^8.0.0-beta.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-minify": "^0.4.3",
    "copy-webpack-plugin": "^4.5.2",
    "dotenv-webpack": "^1.5.7",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "0.10.2",
    "jest": "^23.4.2",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^23.0.1",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "loglevel": "^1.6.1",
    "pixi-filters": "^2.6.1",
    "pixi-multistyle-text": "^0.6.0",
    "pixi-sound": "^2.0.2",
    "pixi.js": "^4.8.1",
    "store": "^2.0.12",
    "webfontloader": "^1.6.28"
  }
}
