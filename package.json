{
  "name": "eternajs",
  "version": "1.10.3",
  "license": "SEE LICENSE IN LICENSE",
  "description": "Eterna game/RNA design interface",
  "scripts": {
    "start": "node multiplatform/mkdirp dist/dev && npm run postbuild:dev && webpack-dev-server --config webpack.dev.js",
    "clean": "node multiplatform/rmrf dist && node multiplatform/rmrf node_modules/.cache",
    "typecheck": "tsc --noEmit",
    "build:dev": "node multiplatform/mkdirp dist/dev && webpack --config webpack.dev.js",
    "build:prod": "node multiplatform/mkdirp dist/prod && webpack --config webpack.prod.js",
    "postbuild:dev": "node multiplatform/cpr ./website/frontend ./dist/dev/frontend && coffee -o ./dist/dev/frontend/jscripts -c ./website/frontend/coffee",
    "test": "jest --verbose",
    "lint": "eslint src --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EteRNAgame/EternaJS.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/base64-js": "^1.2.5",
    "@types/canvas-confetti": "^1.0.0",
    "@types/dompurify": "^2.0.3",
    "@types/jest": "^25.1.4",
    "@types/loglevel": "^1.6.3",
    "@types/node": "^12.12.53",
    "@types/marked": "^1.1.0",
    "@types/storejs": "^2.0.3",
    "@types/upng-js": "^2.1.1",
    "@types/webfontloader": "^1.6.29",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-minify": "^0.5.1",
    "coffeescript": "^1.12.7",
    "css-loader": "^3.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^4.3.0",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-plugin": "^4.0.2",
    "husky": "^3.1.0",
    "jest": "^25.2.3",
    "jest-canvas-mock": "^2.2.0",
    "lint-staged": "^9.5.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "base64-js": "^1.3.1",
    "canvas-confetti": "^1.2.0",
    "dompurify": "^2.0.12",
    "eterna-chat-wrapper": "github:eternagame/eterna-chat-wrapper#f9bc0387c82a325d1846f3b713eefd313b9a91c7",
    "is-mobile": "^2.2.1",
    "js-polyfills": "^0.1.42",
    "libtess": "^1.2.2",
    "localforage": "^1.8.1",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.6.6",
    "marked": "^1.1.1",
    "pchip": "^1.0.2",
    "pixi-filters": "^3.1.1",
    "pixi-multistyle-text": "github:eternagame/pixi-multistyle-text#45a514d8b6234a2a0292f12c8cddebf1b1270a48",
    "pixi-sound": "^3.0.5",
    "pixi.js": "^5.3.3",
    "store": "^2.0.12",
    "upng-js": "^2.1.0",
    "webfontloader": "^1.6.28"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint -- --fix",
      "npm run test -- --findRelatedTests",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run typecheck"
    }
  },
  "optionalDependencies": {
    "eternajs-folding-engines": "github:eternagame/eternajs-folding-engines#06199966f2bf2b0e3756986751a4e53f36a3cef6"
  }
}
